<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_PASSWORD}</password>
            <id>central</id>
        </server>
        <server>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_PASSWORD}</password>
            <id>snapshots</id>
        </server>
        <server>
            <id>vlaamseoverheid-magda-docker-releases.jfrog.io</id>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_PASSWORD}</password>
        </server>
        <server>
            <id>vlaamseoverheid-magda-docker-snapshots.jfrog.io</id>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_PASSWORD}</password>
        </server>
        <server>
            <id>vlaamseoverheid-magda-docker-public.jfrog.io</id>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_PASSWORD}</password>
        </server>

    </servers>

    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>magda-public-release</name>
                    <url>https://vlaamseoverheid.jfrog.io/vlaamseoverheid/magda-public-release</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>snapshots</id>
                    <name>magda-public-snapshot</name>
                    <url>https://vlaamseoverheid.jfrog.io/vlaamseoverheid/magda-public-snapshot</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>magda-public-release</name>
                    <url>https://vlaamseoverheid.jfrog.io/vlaamseoverheid/magda-public-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots/>
                    <id>snapshots</id>
                    <name>magda-public-snapshot</name>
                    <url>https://vlaamseoverheid.jfrog.io/vlaamseoverheid/magda-public-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>
